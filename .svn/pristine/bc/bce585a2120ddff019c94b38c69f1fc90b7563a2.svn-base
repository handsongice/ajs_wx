/* pages/login/login_d.wxss */
.me{
  height: 100%;
}
.userinfo{
  position: relative;
  color: #4b4b4b;
  height: 448rpx;
  display: flex;
  align-items: flex-start;
  justify-content: center;
}
.userinfo-bg{ 
  position: absolute;
  z-index: 1; 
  width: 100%; 
  height: 100%; 
  }

.sc{
  background: #fff;
  width: 100%;
  font-size: 12px;
  line-height: 16px;
  color: #666;
}
.login_qbdd{
  background: #fff;
  width: auto;
  font-size: 12px;
  color: #666;
  padding: 0 7%;
  height: 85rpx;
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.login_qbdd input{
  color: #7b7b7b;
  font-size: 12px;
  height: 85rpx;
  line-height: 85rpx;
}
.login_code,.login_code_1{
  display: flex;
  font-size: 12px;
  padding: 0% 5%;
  justify-content: center;
  background: #4b4b4b;
  color: #fff;
  border-radius: 60rpx;
  align-items: center;
  height: 60rpx;
  white-space: nowrap;
}
.login_code_1{
  background: #9d9d9d;
}
.login{
  display: flex;
  font-size: 14px;
  justify-content: center;
  background: #4b4b4b;
  color: #fff;
  border-radius: 60rpx;
  align-items: center;
  height: 75rpx;
  width: 80%;
  margin: 0 auto;

}
.icon1{
  font-size: 22px;
  color: #7b7b7b;
  line-height: 32px;
}
.icon2{
  font-size: 22px;
  color:rgba(255,255,255,0.5);
  line-height: 30px;
}
.df_1 view{
  position: relative;
}

.w_s{
  white-space: nowrap;
}
.hide{
  display: none;
}
.show{
  display: flex;
}