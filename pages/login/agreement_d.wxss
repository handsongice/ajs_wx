/* pages/login/agreement_d.wxss */
page{
  height: 100%;
  position: relative;
  width: 100%;
  background: #050505;
}
.page {
  font-family: '微软雅黑';
  position: relative;
  height: 100%;

}
.agreement_txt{
  color: #fff;
  font-size:12px;
  padding: 0% 7% 0% 7%; 
  overflow: auto;
  max-height:90%;
}
/*底部按钮*/
.carts-footer {
  width: 100%;
  height: 85rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  position: fixed;
  bottom: 0;
  background: #fff;
  z-index:20;
  color: #7b7b7b;
}
.carts-footer-l{
  display: flex;
  align-items: center;
  justify-content:space-between;
  width: 56%;
}
.carts-footer-l1{
  display: flex;
  align-items: center;

}
/*复选框*/
.carts-footer icon {
  margin-left:20rpx;
  color: #fff;
  margin-right:15rpx;
}
 
/*全选字样*/
.carts-footer text {
  font-size: 26rpx;
}
 
/*确定按钮*/
.carts-footer .button {
  line-height: 85rpx;
  text-align: center;
  width:22%;
  height: 85rpx;
  background-color: #b28147;
  color: white;
  font-size: 30rpx;
  border-radius: 0;
  border: 0;
  font-weight: normal;
}
/*取消*/
.carts-footer .button1{
  line-height: 85rpx;
  text-align: center;
  width:22%;
  height: 85rpx;
  background-color: #4b4b4b;
  color: white;
  font-size: 30rpx;
  border-radius: 0;
  border: 0;
  font-weight: normal;
}